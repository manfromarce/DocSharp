<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <VersionPrefix>0.8.3</VersionPrefix>
    <Product>DocSharp</Product>
    <Authors>Manfredi Marceca</Authors>
    <Copyright>Copyright Â© Manfredi Marceca $([System.DateTime]::Now.Year)</Copyright>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/manfromarce/DocSharp</RepositoryUrl>
    <PackageProjectUrl>https://github.com/manfromarce/DocSharp</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
  
  <ItemGroup>
    <None Include="../../README.md" Pack="true" PackagePath="$(PackageReadmeFile)"/>
  </ItemGroup>
</Project>
